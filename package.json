{
  "name": "webpack-starter-kit",
  "version": "2.1.0",
  "description": "",
  "main": "",
  "scripts": {
    "build": "node lib/clear.js --build && webpack --display-chunks --env.MIN=0 --env.ENV=DEV --env.DEBUG=0",
    "minimize": "node lib/clear.js --build && webpack --display-chunks --optimize-minimize --env.MIN=1 --env.ENV=DEV --env.DEBUG=0",
    "watch": "node lib/clear.js --build && webpack --display-chunks --watch --cache --env.MIN=0 --env.ENV=DEV --env.DEBUG=1",
    "clear": "node lib/clear.js --all",
    "test": "node lib/clear.js --build && webpack --display-chunks --env.MIN=1 --env.ENV=TEST --env.DEBUG=0",
    "distribute": "node lib/clear.js --dist && webpack --display-chunks --env.MIN=0 --env.ENV=PROD --env.DEBUG=0 && webpack --display-chunks --optimize-minimize --env.MIN=2 --env.ENV=PROD --env.DEBUG=0 && node lib/copy.js --config copy",
    "production": "node lib/clear.js --dist && webpack --display-chunks --optimize-minimize --env.MIN=1 --env.ENV=PROD --env.DEBUG=0 && node lib/concat.js --config concat",
    "server": "node lib/server.js --dev",
    "server-prod": "node lib/server.js --prod"
  },
  "keywords": [
    "webpack",
    "starter",
    "kit"
  ],
  "author": "Adrian Gargula",
  "license": "ISC",
  "repository": "git@bitbucket.org:Adrosar/webpack-starter-kit.git",
  "dependencies": {
    "express": "~4.16.2",
    "serve-index": "~1.9.1"
  },
  "devDependencies": {
    "@types/jquery": "~2.0.49",
    "@types/node": "~8.9.4",
    "autoprefixer": "^7.2.6",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.4",
    "babel-preset-env": "~1.6.1",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "base64-image-loader": "~1.2.1",
    "bundle-loader": "^0.5.6",
    "css-loader": "^0.28.10",
    "do-nothing-loader": "~1.0.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.11",
    "fs-extra": "~5.0.0",
    "html-minify-loader": "~1.3.0",
    "json-loader": "^0.5.7",
    "less": "~3.0.0",
    "less-loader": "~4.0.6",
    "node-sass": "~4.7.2",
    "postcss-loader": "~2.1.1",
    "preprocess-loader": "^0.2.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "~6.0.7",
    "style-loader": "^0.20.2",
    "ts-loader": "~3.5.0",
    "typescript": "~2.7.2",
    "uglify-loader": "~2.0.0",
    "webpack": "~3.11.0",
    "xml-loader": "~1.2.1"
  }
}
