{
    "name": "webpack-starter-kit",
    "version": "3.0.0-beta3",
    "license": "MIT",
    "contributors": [
        {
            "name": "Adrian Gargula",
            "email": "adriangargula@interia.pl"
        }
    ],
    "scripts": {
        "watch": "webpack --config webpack.config.js --mode development --watch",
        "live": "webpack --config webpack.config.js --mode development --watch --liveReload",
        "build": "webpack --config webpack.config.js --mode development",
        "min": "webpack --config webpack.config.js --mode production --forDEV",
        "dist": "webpack --config webpack.config.js --mode production",
        "dev": "webpack-dev-server --forDEV --disableNodeJs",
        "server": "node ./lib/server.js --port 8080",
        "typedoc": "typedoc --options typedoc.config.js",
        "clear": "node ./lib/clear.js"
    },
    "engines": {
        "node": "10.x.x"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@types/express": "^4.17.1",
        "@types/node": "^10.15.3",
        "autoprefixer": "^9.7.0",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^5.0.4",
        "css-loader": "^3.2.0",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "preprocess-loader": "^0.3.0",
        "raw-loader": "^2.0.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.0",
        "terser-webpack-plugin": "^2.2.1",
        "ts-loader": "^6.2.1",
        "typedoc": "^0.15.0",
        "typescript": "^3.6.4",
        "uglifycss": "^0.0.29",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.9.0",
        "webpack-livereload-plugin": "^2.2.0",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "express": "^4.17.1",
        "http-server": "^0.11.1",
        "nocache": "^2.1.0",
        "serve-index": "^1.9.1"
    }
}
